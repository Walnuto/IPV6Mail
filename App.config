<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.8" />
    </startup>
	<appSettings>
		<add key="smtpaddr" value="smtp.qq.com"/>
		<add key="smtpport" value="587"/>
		<add key="username" value="yourmail@qq.com"/>
		<add key="password" value="password"/>
		<add key="mailfrom" value="yourmail@qq.com"/>
		<add key="mailrcpt" value="yourmail@qq.com"/>
		<add key="interval" value="60"/>
	</appSettings>
</configuration>